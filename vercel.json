{
  "version": 2,
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/privacy",
      "dest": "/privacy/index.html"
    },
    {
      "src": "/legal",
      "dest": "/legal/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
